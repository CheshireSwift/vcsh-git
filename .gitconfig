[user]
  email = swift@swifteagle.co.uk
  name = Sam Collard

[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[core]
  #autocrlf = false
  #filemode = false
  #filemod = false
  #whitespace = cr-at-eol
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  head = !git r -1
  h = !git head
  hp = "!. ~/.githelpers && show_git_head"
  r = !GIT_NO_PAGER=1 git l -30
  ra = !git r --all
  l = "!. ~/.githelpers && pretty_git_log"
  la = !git l --all[difftool "doc"]
  ir = "!. ~/.githelpers && interactive_rebase"
  sq = !git ir 2
  ls = status
  fleece = push --force-with-lease
[credential]
  helper = store
[push]
  default = simple
[color "diff"]
  meta = 81
  frag = 208
  old = 161
  new = 118

